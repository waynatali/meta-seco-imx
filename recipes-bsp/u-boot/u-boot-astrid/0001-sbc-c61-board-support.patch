+	ENV_RAMFS                                                         \
+	ENV_ROOT                                                          \
+	ENV_PREBOOT                                                       \
+	"bootcmd=" ENV_CMD_SET_BOOT "\0"
+
+
+#endif     /*  __SECO_ENVIRONMENT_H  */
