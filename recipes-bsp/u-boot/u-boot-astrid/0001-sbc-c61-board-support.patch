+
+#endif  /* CONFIG_OF_LIBFDT_OVERLAY */
+
+
+
+#define ENV_RAMFS                                                     \
+	"ramfs_use=0\0"                                                   \
+	"ramfs_device_id="__stringify(ENV_SYS_MMC_ENV_DEV)"\0"            \
